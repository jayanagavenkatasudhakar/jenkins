pipeline
{
	agent 'none'
	stages
	{		
		stage(Pulling_to_Mythili_PC)
		{	
			agent {node 'mythili_agent'}
			steps
			{
				git 'https://github.com/jayanagavenkatasudhakar/jenkins.git'
				echo 'source code pulling sucessfully @ Myhili PC'
			}
		}
		stage(transfer_to_ftp_server)
		{
			build job:'ftp_files_transfer'
		}
				
	}

}
