pipeline
{
	agent any
	
	stages
	{		
		stage(Pulling_to_My_PC)
		{	
			
			steps
			{
				git 'https://github.com/jayanagavenkatasudhakar/jenkins.git'
				echo 'source code pulling sucessfully to My(Jaya) PC'
				func1()
				empname("jaya NAGA VENKATA SUDHAKAR.Bavirisetty")
			}
		}
			
	}

}
void func1()
{
	echo 'Inside func1>>>>>>>>>>>>>>><<<<<<<<<<<<<<<<<<<'
}
def empname(String a)
{
	echo " Emp name:"
	println(a)
	
}
